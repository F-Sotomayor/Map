arker.addListener('click', function(event) {\r\n    if (infowindowContent.open === false) {\r\n       infowindow.open(map, marker);\r\n       infowindowContent.open = true;\r\n       }\r\n   else {\r\n      infowindow.close()\r\n      infowindowContent.open = false\r\n   }\r\n\r\n  })\r\n \r\n}"},"lineCount":null}},"error":null,"hash":"f1e3c6e854723c63ccf28d7dc8042284","cacheData":{"env":{}}}